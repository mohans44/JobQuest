{
  "manifest_version": 3,
  "name": "JobQuest",
  "version": "1.1",
  "description": "Track all your job applications at one place",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
      "48": "icons/logo.png"
  }
}
